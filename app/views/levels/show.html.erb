<% title "Level" %>

<p>
  <strong>Name:</strong>
  <%= @level.name %>
</p>
<p>
  <strong>Description:</strong>
  <%= @level.description %>
</p>
<p>
  <strong>Image Url:</strong>
  <%= @level.image_url %>
</p>

<p>
  <%= link_to "Edit", edit_level_path(@level) %> |
  <%= link_to "Destroy", @level, :confirm => 'Are you sure?', :method => :delete %> |
  <%= link_to "View All", levels_path %>
</p>
